@model IEnumerable<ASP.Net_MVC_Fundamentals.Models.Customer>
@{
    ViewBag.Title = "Index";
}

<h2>Customers</h2>

@if (@Model.Count()== 0)
{
    <text>We dont have customers yet </text>
}
else
{
    <ul>
        @foreach (var customer in @Model)
        {
            <li>@customer.Name</li>
        }

    </ul>
}

